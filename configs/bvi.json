{
    "seed": 42,
    "save_dir": "checkpoints/",
    "data_loader": {
        "name": "BVI",
        "data_root": "./dataset_copy/low_light_10",
        "w": 432,
        "h": 240,
        "sample_length": 5
    },
    "losses": {
        "hole_weight": 1,
        "valid_weight": 1,
        "adversarial_weight": 0.01,
        "GAN_LOSS": "hinge"
    },
    "model": {
        "net": "fuseformer",
        "no_dis": 1
    },
    "trainer": {
        "type": "Adam",
        "beta1": 0.5,
        "beta2": 0.99,
        "lr": 1e-5,
        "batch_size": 8,
        "num_workers": 4,
        "log_freq": 20,
        "save_freq": 200,
        "iterations": 25000,
        "niter": 5000
    }
}
